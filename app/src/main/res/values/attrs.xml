<?xml version="1.0" encoding="utf-8"?>
<resources>



    <declare-styleable name="MyRadioButton">

        <attr name="drawableHeightSize" format="dimension" />
        <attr name="drawableTop" format="reference" />
        <attr name="drawableLeft" format="reference" />
        <attr name="drawableRight" format="reference" />
        <attr name="drawableBottom" format="reference" />
    </declare-styleable>

    <declare-styleable name="DotView">
        <attr name="lineColor" format="reference|color"></attr>
        <attr name="lineWidth" format="reference|integer"></attr>
        <attr name="dotNum" format="reference|integer"></attr>
        <attr name="dotWidth" format="reference|integer"></attr>
        <attr name="lineOriental">
            <enum name="horizontal" value="0"></enum>
            <enum name="vertical" value="1"></enum>
        </attr>
    </declare-styleable>

</resources>
